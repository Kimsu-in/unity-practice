using UnityEngine;
using System.Collections;

public class Manager : MonoBehaviour {
	public int Count;
	public float _time;
	bool End;

	void OnTriggerEnter(Collider Get){
				if (Get.collider.tag == "Box") {
						Count += 1;
				}
				if (Count >= 16) {
						End = true;
				}
		}

	// Update is called once per frame
	void Update () {
		if (End == false) {
						_time += Time.deltaTime;
				}
	}
}
